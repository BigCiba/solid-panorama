        "prettier": "^2.8.4",
        "rollup": "^3.17.2",
        "rollup-plugin-copy": "^3.4.0",
        "rollup-plugin-terser": "^7.0.2",
        "solid-js": "^1.6.11",
        "ts-jest": "^29.0.5",
        "ts-node": "^10.9.1",
        "tslib": "^2.5.0",
        "typescript": "^4.9.5",
        "xml-js": "^1.6.11"
